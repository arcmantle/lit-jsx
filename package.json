{
	"name": "@arcmantle/lit-jsx",
	"version": "1.0.0",
	"description": "A JSX runtime and compiler that transforms JSX into Lit templates.",
	"repository": {
		"type": "git",
		"url": "https://github.com/arcmantle/lit-jsx.git"
	},
	"license": "Apache-2.0",
	"author": "Kristoffer Roen-Lie",
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": "./dist/utils.js",
		"./jsx-runtime": "./dist/shared/jsx-types.d.ts",
		"./jsx-dev-runtime": "./dist/shared/jsx-types.d.ts",
		"./vite": "./dist/compiler/vite-plugin.js"
	},
	"main": "./dist/react-jsx/jsx-runtime.js",
	"types": "./dist/shared/jsx-types.d.ts",
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"build": "rimraf dist && tsc --project ./src/tsconfig.json",
		"dev": "vite build --watch --mode=development",
		"test": "pnpm vitest run"
	},
	"dependencies": {
		"@babel/core": "catalog:",
		"@babel/plugin-syntax-jsx": "catalog:",
		"@babel/traverse": "catalog:",
		"@babel/types": "catalog:",
		"@types/babel__core": "catalog:",
		"@types/babel__traverse": "catalog:",
		"csstype": "catalog:",
		"deepmerge-ts": "catalog:",
		"lit-html": "catalog:",
		"oxc-resolver": "catalog:",
		"validate-html-nesting": "catalog:"
	},
	"devDependencies": {
		"oxc-parser": "catalog:",
		"oxc-walker": "catalog:",
		"typescript": "catalog:",
		"vite": "catalog:",
		"vitest": "catalog:"
	}
}
